@namespace("keybase.1")
protocol NotifyWot {
  import idl "common.avdl";

  // Notification from gregor
  record WotChangedMsg {
    @jsonkey("voucherUid")
    union { null, UID } voucherUID;
    @jsonkey("voucheeUid")
    union { null, UID } voucheeUID;
  }

  void wotChanged(string category, union { null, UID } voucher, union { null, UID } vouchee) oneway;
}
